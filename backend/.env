# Environment
NODE_ENV=development

# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:5000

# Database Configuration - Using MySQL
# Replace YOUR_EC2_IP with your actual EC2 server IP address
DB_HOST=YOUR_EC2_IP
DB_PORT=3306
DB_USER=mygroup
DB_PASSWORD=MyGroup@2025!
DB_NAME=my_group

# Security
JWT_SECRET=your-super-secret-jwt-key-for-apphub-development
SESSION_SECRET=your-super-secret-session-key-for-apphub-development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
AUTH_RATE_LIMIT_MAX=10